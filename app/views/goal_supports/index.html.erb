<%- model_class = GoalSupport -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:i_support) %></th>
      <th><%= model_class.human_attribute_name(:goal_id) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @goal_supports.each do |goal_support| %>
      <tr>
        <td><%= link_to goal_support.id, goal_support_path(goal_support) %></td>
        <td><%= goal_support.i_support %></td>
        <td><%= goal_support.goal_id %></td>
        <td><%= goal_support.user_id %></td>
        <td><%=l goal_support.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_goal_support_path(goal_support), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      goal_support_path(goal_support),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_goal_support_path,
            :class => 'btn btn-primary' %>
