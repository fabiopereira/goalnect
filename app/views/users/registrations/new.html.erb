<script>	
	  $(document).ready(function(){
		 $("#dob").datepicker({changeYear : true, changeMonth : true});
	  });
</script>
<h2><%= t 'devise.sessions.sign_up'%></h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= render 'layouts/model_error_messages', :target => resource %>


  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>
  
    <div><%= f.label :screen_name %><br />
  <%= f.text_field :screen_name %></div>
  
    <div>www.goalnect.com.au/<br />
  <%= f.text_field :username %></div>

  <div class="control-group">
    <%= f.label :dob, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :dob, :class => 'date_select', :id => 'dob' %>
    </div>
  </div>

 <div class="control-group">
    <%= f.label :country %>
    <div class="controls">
     <%= collection_select(:user, :country_id, Country.all, :id, :name) %>
    </div>
  </div>


  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render :partial => "devise/shared/links" %>
 