<%- model_class = GoalSupport -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:i_support) %>:</strong></dt>
  <dd><%= @goal_support.i_support %></dd>
  <dt><strong><%= model_class.human_attribute_name(:goal_id) %>:</strong></dt>
  <dd><%= @goal_support.goal_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @goal_support.user_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              goal_support_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_goal_support_path(@goal_support), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              goal_support_path(@goal_support),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
